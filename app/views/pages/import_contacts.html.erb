<div class="row">
    <div class="col-lg-12">
        <h1 class="page-header">
            Import Contacts
        </h1>
    </div>
</div>

<h4>Download Links:</h4>
<p><%= link_to "Excel", user_contacts_path(@user, format: "xls") %> | <%= link_to "CSV", user_contacts_path(@user, format: "csv") %></p>

<h2>Import Contacts</h2>
<%= form_tag import_user_contacts_path(@user), multipart: true do %>
  <%= file_field_tag :file %>
  <%= submit_tag "Import" %>
<% end %>